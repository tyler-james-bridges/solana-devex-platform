{
  "name": "solana-devex-platform",
  "version": "1.0.0",
  "description": "Complete development environment and dashboard for Solana applications",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:all": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "lint:api": "cd api && eslint . --ext .js,.ts --max-warnings 0",
    "lint:cli": "cd cli && eslint . --ext .js,.ts --max-warnings 0",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,yaml,yml}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,yaml,yml}\"",
    "type-check": "tsc --noEmit",
    "test": "npm run test:api",
    "test:api": "cd api && npm test",
    "test:protocols": "solana-devex test protocols",
    "test:agentdex": "node tests/test-agentdex-integration.js",
    "test:coverage": "cd api && npm test -- --coverage",
    "security:audit": "npm audit && cd api && npm audit && cd ../cli && npm audit",
    "security:fix": "npm audit fix && cd api && npm audit fix && cd ../cli && npm audit fix",
    "quality:check": "npm run type-check && npm run lint:all && npm run format:check && npm run test",
    "quality:fix": "npm run format && npm run lint:fix",
    "api": "cd api && npm start",
    "api:dev": "cd api && npm run dev",
    "cli:install": "cd cli && npm install && npm run install-global",
    "setup": "npm install && cd api && npm install && cd ../cli && npm install",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "lighthouse": "lhci autorun",
    "bundle-size": "bundlesize",
    "cicd:start": "node scripts/start-cicd-system.js",
    "cicd:setup-project": "node integrations/project-cicd-setup.js",
    "cicd:webhook": "node webhooks/standalone-webhook.js",
    "cicd:server": "cd api && node start-cicd-server.js",
    "cicd:dashboard": "npm run dev",
    "cicd:all": "concurrently \"npm run cicd:server\" \"npm run cicd:dashboard\"",
    "makora:setup": "npm run cicd:setup-project -- /path/to/makora",
    "solprism:setup": "npm run cicd:setup-project -- /path/to/solprism",
    "production:start": "NODE_ENV=production node api/production-server.js",
    "production:cluster": "NODE_ENV=production node api/production-server.js --cluster",
    "load:test": "node production-load-test.js",
    "load:test:full": "TEST_AGENTS=100 TEST_DURATION=600000 node production-load-test.js",
    "monitor": "node api/production-monitor.js",
    "health:check": "node -e \"console.log('Health check would run here')\"",
    "production:setup": "chmod +x setup-production.sh && ./setup-production.sh",
    "db:migrate": "echo 'Database migration placeholder'",
    "db:seed": "echo 'Database seeding placeholder'"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.0",
    "@solana/web3.js": "^1.95.0",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "axios": "^1.13.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "critters": "^0.0.23",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "lru-cache": "^10.1.0",
    "lucide-react": "^0.303.0",
    "next": "14.2.35",
    "pg": "^8.11.3",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.8.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@lhci/cli": "^0.12.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/pg": "^8.10.9",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "bundlesize": "^0.18.2",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "eslint-plugin-security": "^1.7.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1"
  },
  "keywords": [
    "solana",
    "devex",
    "ci-cd",
    "testing",
    "dashboard",
    "defi"
  ],
  "author": "onchain-devex",
  "license": "MIT"
}
