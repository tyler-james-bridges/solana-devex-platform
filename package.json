{
  "name": "solana-devex-platform",
  "version": "1.0.0",
  "description": "Unified Solana Developer Experience Platform - Complete toolkit with testing, monitoring, CI/CD, and enhanced development tools",
  "main": "index.js",
  "bin": {
    "solana-devex": "./bin/solana-devex"
  },
  "scripts": {
    "platform:setup": "node bin/solana-devex setup",
    "platform:init": "node bin/solana-devex config init",
    "platform:test": "node bin/solana-devex test",
    "platform:build": "node bin/solana-devex build",
    "platform:validator": "node bin/solana-devex validator start",
    "platform:monitor": "node bin/solana-devex monitor start",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:all": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "lint:api": "cd api && eslint . --ext .js,.ts --max-warnings 0",
    "lint:cli": "cd cli && eslint . --ext .js,.ts --max-warnings 0",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,yaml,yml}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,yaml,yml}\"",
    "type-check": "tsc --noEmit",
    "test": "npm run test:api",
    "test:api": "cd api && npm test",
    "test:protocols": "solana-devex test protocols",
    "test:agentdex": "node tests/test-agentdex-integration.js",
    "test:coverage": "cd api && npm test -- --coverage",
    "security:audit": "npm audit && cd api && npm audit && cd ../cli && npm audit",
    "security:fix": "npm audit fix && cd api && npm audit fix && cd ../cli && npm audit fix",
    "quality:check": "npm run type-check && npm run lint:all && npm run format:check && npm run test",
    "quality:fix": "npm run format && npm run lint:fix",
    "api": "cd api && npm start",
    "api:dev": "cd api && npm run dev",
    "integrations": "node api/integration-apis.js",
    "integrations:dev": "nodemon api/integration-apis.js",
    "demo": "node api/demo-environment.js",
    "demo:dev": "nodemon api/demo-environment.js",
    "cli:install": "cd cli && npm install && npm run install-global",
    "setup": "npm install && cd api && npm install && cd ../cli && npm install",
    "start:all": "concurrently \"npm run dev\" \"npm run integrations\" \"npm run demo\" \"npm run api\"",
    "start:production": "npm run build && npm start && npm run integrations && npm run demo && npm run api",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "lighthouse": "lhci autorun",
    "bundle-size": "bundlesize",
    "cicd:start": "node scripts/start-cicd-system.js",
    "cicd:setup-project": "node integrations/project-cicd-setup.js",
    "cicd:webhook": "node webhooks/standalone-webhook.js",
    "cicd:server": "cd api && node start-cicd-server.js",
    "cicd:dashboard": "npm run dev",
    "cicd:all": "concurrently \"npm run cicd:server\" \"npm run cicd:dashboard\"",
    "makora:setup": "npm run cicd:setup-project -- /path/to/makora",
    "solprism:setup": "npm run cicd:setup-project -- /path/to/solprism",
    "production:start": "NODE_ENV=production node api/production-server.js",
    "production:cluster": "NODE_ENV=production node api/production-server.js --cluster",
    "load:test": "node production-load-test.js",
    "dev:monitor": "node scripts/dev-monitor.js start",
    "dev:monitor:stop": "node scripts/dev-monitor.js stop",
    "dev:monitor:setup": "node scripts/dev-monitor.js setup",
    "monitor:server": "node api/dev-monitor-server.js",
    "load:test:full": "TEST_AGENTS=100 TEST_DURATION=600000 node production-load-test.js",
    "monitor": "node api/production-monitor.js",
    "health:check": "node -e \"console.log('Health check would run here')\"",
    "production:setup": "chmod +x setup-production.sh && ./setup-production.sh",
    "db:migrate": "echo 'Database migration placeholder'",
    "db:seed": "echo 'Database seeding placeholder'",
    "collaboration:demo": "node scripts/start-collaboration-demo.js",
    "collaboration:ws": "node api/collaboration-websocket.js"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.0",
    "@solana/spl-token": "^0.4.0",
    "@solana/web3.js": "^1.95.0",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "axios": "^1.13.4",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.11.0",
    "bn.js": "^5.2.1",
    "boxen": "^5.1.2",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "commander": "^11.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "critters": "^0.0.23",
    "cross-spawn": "^7.0.3",
    "execa": "^5.1.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "glob": "^10.3.10",
    "helmet": "^7.1.0",
    "inquirer": "^8.2.6",
    "ioredis": "^5.3.2",
    "lru-cache": "^10.1.0",
    "lucide-react": "^0.303.0",
    "nanoid": "^3.3.4",
    "next": "14.2.35",
    "node-cron": "^3.0.2",
    "ora": "^5.4.1",
    "pg": "^8.11.3",
    "pidusage": "^3.0.2",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.8.0",
    "semver": "^7.5.4",
    "socket.io": "^4.7.5",
    "systeminformation": "^5.17.12",
    "table": "^6.8.1",
    "tailwindcss": "^3.3.6",
    "toml": "^3.0.0",
    "typescript": "^5.3.3",
    "ws": "^8.19.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@lhci/cli": "^0.12.0",
    "@types/blessed": "^0.1.21",
    "@types/bn.js": "^5.1.1",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.11",
    "@types/pg": "^8.10.9",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "bundlesize": "^0.18.2",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-security": "^1.7.1",
    "fs-extra": "^11.3.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "vitest": "^1.0.4"
  },
  "keywords": [
    "solana",
    "devex",
    "infrastructure",
    "monitoring",
    "partnerships",
    "enterprise",
    "agents",
    "colosseum-hackathon",
    "api-monitoring",
    "protocol-health",
    "ci-cd",
    "testing",
    "dashboard",
    "defi",
    "agentdex",
    "solprism",
    "multi-network",
    "ecosystem"
  ],
  "author": "onchain-devex",
  "license": "MIT"
}
